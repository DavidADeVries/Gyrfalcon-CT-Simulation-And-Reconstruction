classdef SliceData
    % SliceData
    % Holds data for the imaging of a slice
    %
    % *angleData
    % a cell array, with each index containing an AngleData
    %
    % *slicePosition
    %
    
    properties
        angleData
        slicePosition
    end
    
    methods
        function data = SliceData(angleData, slicePosition)
            data.angleData = angleData;
            data.slicePosition = slicePosition;
        end
    end
    
end

